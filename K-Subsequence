class Result{
 
public static long kSub(int k, List<Integer>nums){
int[] sum= new int [nums.size()];
long count =0;
sums[0] = nums.get(0);
for(int i=1; i<nums.size(); i++){
sum[i]= sum[i-1]+nums.get(i);
}
int[] val = nw int[k];
for(int i=0;i<sum.length;i++){
int mod = sum[i]%k;
if(mod==0)
count++;
count+= val[mod];
val[mod]+=1;
}
return count;
}
}
